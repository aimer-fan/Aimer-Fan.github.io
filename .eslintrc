{
    "root": true,
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "@aimerfan",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": [
        "!docs/.vitepress",
        "!components/**/*.vue",
        "docs/.vitepress/cache",
        "docs/.vitepress/dist"
    ]
}
